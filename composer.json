{
    "name": "xpate-payments/xpate-php",
    "description": "The official Xpate Payments PHP SDK",
    "keywords": ["payment", "pay", "merchant", "e-commerce"],
    "license": "MIT",
    "homepage": "https://github.com/xpate-payments/xpate-php",
    "authors": [
        {
            "name": "Xpate Payments",
            "email": "dev@gingerpayments.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.0 || ^9.0",
        "doctrine/instantiator": "^1.0.5 || ^1.1.0 || ^1.2.0",
        "phpdocumentor/reflection-common": "^1.0 || ^2.0",
        "phpdocumentor/reflection-docblock": "^4.0 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Xpate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xpate\\Tests\\": "tests"
        }
    }
}
